<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wkcto.crm.workbench.dao.TranHistoryDao">
    <insert id="save" parameterType="TranHistory">
        insert into
         tbl_tran_history
         (id,stage,money,expectedDate,createTime,createBy,tranId)
         values
         (#{id},#{stage},#{money},#{expectedDate},#{createTime},#{createBy},#{tranId})
    </insert>
    <select id="getList" resultType="TranHistory">
        select
        *
        from
        tbl_tran_history th
        where
        th.tranId = #{tranId}
        order by
        createTime desc
    </select>
    <delete id="delByTranIdArray">
        delete from
        tbl_tran_history
        where tranId in
        <foreach collection="array" item="o" open="(" separator="," close=")">
            #{o}
        </foreach>
    </delete>
</mapper>