<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wkcto.crm.workbench.dao.ChartDao">
    <select id="getTranChart" resultType="Map">
        select
          count(*) as value,stage as name
        from
        tbl_tran
        group by
        stage
    </select>
</mapper>